{
  "compilerOptions": {
    "target": "es5",                                
    "module": "commonjs",                           
    "strict": true,                                 
    "esModuleInterop": true,   
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,                     
    "skipLibCheck": true,  
    "sourceMap": true,                         
    "forceConsistentCasingInFileNames": true,
    "outDir": "lib"
  }
}
